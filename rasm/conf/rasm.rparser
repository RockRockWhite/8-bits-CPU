%{
    enum Instruction {
        MV = 0,
        LB = 1,
        LI = 2,
        SB = 3,

        ADD = 10,
        ADDI = 11,
        SUB = 12,
        AND = 13,
        ANDI = 14,
        OR = 15,
        ORI = 16,
        XOR = 17,
        XORI = 18,
        NOT = 19,

        CMP = 32,
        JAL = 33,
        JALR = 34,
        BE = 35,
        BNE = 36,
        BGE = 37,
        BLT = 38,

        NOP = 62,
        HLT = 63,
    }

    enum Register {
        R0 = 8,
        R1 = 9,
        R2 = 10,
        R3 = 11,
        R4 = 12,
        R5 = 13,
        R6 = 14,
        R7 = 15,
    }

    impl Register {
        pub fn from_str(s: &str) -> Option<Self> {
            match s {
                "r0" => Some(Register::R0),
                "r1" => Some(Register::R1),
                "r2" => Some(Register::R2),
                "r3" => Some(Register::R3),
                "r4" => Some(Register::R4),
                "r5" => Some(Register::R5),
                "r6" => Some(Register::R6),
                "r7" => Some(Register::R7),
                _ => None,
            }
        }
    }
%}

